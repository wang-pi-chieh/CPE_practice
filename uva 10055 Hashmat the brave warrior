#include <stdio.h>
#include <stdlib.h>
#pragma warning (disable:4996)

int main() {
	long long Hashmat = 0;
	long long oppnent =0; //Hashmat is Hashmat's army number, oppnent is his oppnent's army number

	while (scanf("%lld %lld", &Hashmat, &oppnent) != EOF) {
		long long temp = Hashmat - oppnent > 0 ? Hashmat - oppnent : oppnent - Hashmat;
		printf("%lld\n", temp);
	}

	return 0;
	
}