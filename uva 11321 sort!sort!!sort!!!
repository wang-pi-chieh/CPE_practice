#include <iostream>
using namespace std;

int main() {
	int number1;
	cout << "Enter first integer: ";
	cin >> number1;

	int number2;
	int number3;
	int sum;

	cout << "Enter second integer: ";
	cin >> number2;
	cout << "Enter third integer: ";
	cin >> number3;
	sum = number1 + number2 + number3;
	cout << "Sum is " << sum << endl;

	return 0;

}

